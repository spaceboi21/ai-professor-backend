{
  "info": {
    "name": "Internship API",
    "description": "Complete API collection for testing Internship feature",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json",
    "_postman_id": "internship-api-v1"
  },
  "item": [
    {
      "name": "School Admin - Internship Management",
      "item": [
        {
          "name": "1. Create new internship",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{token}}"
              },
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"title\": \"Clinical Psychology Internship - Year 3\",\n  \"description\": \"This internship provides hands-on experience in clinical psychology with real patient cases. Students will practice assessment, diagnosis, and therapy techniques under AI supervision.\",\n  \"guidelines\": \"<h2>Guidelines for Students</h2><ul><li>Complete all cases in sequence</li><li>Spend minimum 30 minutes per patient interview</li><li>Document all observations in your logbook</li><li>Real-time supervision available</li><li>Professor validation required for feedback</li></ul>\",\n  \"year\": 3,\n  \"thumbnail\": \"https://your-s3-bucket.s3.amazonaws.com/internship-thumbnails/clinical-psych.jpg\",\n  \"duration\": 40\n}",
              "options": {
                "raw": {
                  "language": "json"
                }
              }
            },
            "url": {
              "raw": "{{baseUrl}}/api/internship",
              "host": ["{{baseUrl}}"],
              "path": ["api", "internship"]
            },
            "description": "Creates a new internship. Requires SCHOOL_ADMIN or PROFESSOR role."
          },
          "response": []
        },
        {
          "name": "2. Get all internships",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{token}}"
              }
            ],
            "url": {
              "raw": "{{baseUrl}}/api/internship?page=1&limit=10&year=3&published=true&search=clinical",
              "host": ["{{baseUrl}}"],
              "path": ["api", "internship"],
              "query": [
                {
                  "key": "page",
                  "value": "1",
                  "description": "Page number (default: 1)"
                },
                {
                  "key": "limit",
                  "value": "10",
                  "description": "Items per page (default: 10)"
                },
                {
                  "key": "year",
                  "value": "3",
                  "description": "Filter by year (1-5)"
                },
                {
                  "key": "published",
                  "value": "true",
                  "description": "Filter by published status",
                  "disabled": true
                },
                {
                  "key": "search",
                  "value": "clinical",
                  "description": "Search in title/description",
                  "disabled": true
                }
              ]
            },
            "description": "Get paginated list of internships with optional filters"
          },
          "response": []
        },
        {
          "name": "3. Get internship by ID",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{token}}"
              }
            ],
            "url": {
              "raw": "{{baseUrl}}/api/internship/:id",
              "host": ["{{baseUrl}}"],
              "path": ["api", "internship", ":id"],
              "variable": [
                {
                  "key": "id",
                  "value": "{{lastInternshipId}}",
                  "description": "Internship MongoDB ObjectId"
                }
              ]
            },
            "description": "Get detailed information about a specific internship"
          },
          "response": []
        },
        {
          "name": "4. Update internship",
          "request": {
            "method": "PATCH",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{token}}"
              },
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"title\": \"Advanced Clinical Psychology Internship - Updated\",\n  \"description\": \"Updated description with more details\",\n  \"duration\": 50,\n  \"guidelines\": \"<h2>Updated Guidelines</h2><p>New requirements...</p>\"\n}",
              "options": {
                "raw": {
                  "language": "json"
                }
              }
            },
            "url": {
              "raw": "{{baseUrl}}/api/internship/:id",
              "host": ["{{baseUrl}}"],
              "path": ["api", "internship", ":id"],
              "variable": [
                {
                  "key": "id",
                  "value": "{{lastInternshipId}}"
                }
              ]
            },
            "description": "Update existing internship (partial update)"
          },
          "response": []
        },
        {
          "name": "5. Delete internship (soft delete)",
          "request": {
            "method": "DELETE",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{token}}"
              }
            ],
            "url": {
              "raw": "{{baseUrl}}/api/internship/:id",
              "host": ["{{baseUrl}}"],
              "path": ["api", "internship", ":id"],
              "variable": [
                {
                  "key": "id",
                  "value": "{{lastInternshipId}}"
                }
              ]
            },
            "description": "Soft delete internship and all related data"
          },
          "response": []
        },
        {
          "name": "6. Publish internship",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{token}}"
              },
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"internship_id\": \"{{lastInternshipId}}\",\n  \"action\": \"PUBLISH\"\n}",
              "options": {
                "raw": {
                  "language": "json"
                }
              }
            },
            "url": {
              "raw": "{{baseUrl}}/api/internship/toggle-visibility",
              "host": ["{{baseUrl}}"],
              "path": ["api", "internship", "toggle-visibility"]
            },
            "description": "Publish internship to students. Sends notifications."
          },
          "response": []
        },
        {
          "name": "7. Unpublish internship",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{token}}"
              },
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"internship_id\": \"{{lastInternshipId}}\",\n  \"action\": \"UNPUBLISH\"\n}",
              "options": {
                "raw": {
                  "language": "json"
                }
              }
            },
            "url": {
              "raw": "{{baseUrl}}/api/internship/toggle-visibility",
              "host": ["{{baseUrl}}"],
              "path": ["api", "internship", "toggle-visibility"]
            },
            "description": "Unpublish internship from students"
          },
          "response": []
        }
      ]
    },
    {
      "name": "School Admin - Case Management",
      "item": [
        {
          "name": "1. Create case for internship",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{token}}"
              },
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"title\": \"Major Depressive Disorder - Initial Assessment\",\n  \"description\": \"Patient John, 35-year-old male, presents with 6-week history of depressed mood, anhedonia, and psychomotor retardation.\",\n  \"sequence\": 1,\n  \"case_content\": \"<h2>Patient Presentation</h2><p><strong>Chief Complaint:</strong> \\\"I just don't feel like myself anymore.\\\"</p><h3>History of Present Illness</h3><p>Patient is a 35-year-old male presenting with a 6-week history of persistent depressed mood.</p>\",\n  \"patient_simulation_config\": {\n    \"patient_profile\": {\n      \"name\": \"John Smith\",\n      \"age\": 35,\n      \"gender\": \"male\",\n      \"condition\": \"Major Depressive Disorder\",\n      \"severity\": \"moderate\",\n      \"duration\": \"6 weeks\",\n      \"triggers\": [\"job loss\", \"financial stress\"],\n      \"symptoms\": [\"depressed mood\", \"anhedonia\", \"sleep disturbances\", \"decreased appetite\"],\n      \"history\": \"One previous depressive episode 5 years ago\",\n      \"medication\": \"Previously on sertraline 100mg\",\n      \"social_context\": \"Married, two children, recently unemployed\",\n      \"chief_complaint\": \"I just don't feel like myself anymore\",\n      \"presentation_style\": \"withdrawn, soft-spoken\"\n    },\n    \"scenario_type\": \"initial_clinical_interview\",\n    \"difficulty_level\": \"intermediate\",\n    \"interview_focus\": \"assessment_and_diagnosis\",\n    \"patient_openness\": \"moderately_forthcoming\"\n  },\n  \"supervisor_prompts\": [\"Monitor empathy\", \"Check suicide risk assessment\"],\n  \"therapist_prompts\": [\"Guide on therapeutic alliance\", \"Teach CBT techniques\"],\n  \"evaluation_criteria\": [\n    {\"criterion\": \"Clinical Reasoning\", \"weight\": 30},\n    {\"criterion\": \"Communication Skills\", \"weight\": 25},\n    {\"criterion\": \"Mental Status Exam\", \"weight\": 20}\n  ]\n}",
              "options": {
                "raw": {
                  "language": "json"
                }
              }
            },
            "url": {
              "raw": "{{baseUrl}}/api/internship/:internshipId/cases",
              "host": ["{{baseUrl}}"],
              "path": ["api", "internship", ":internshipId", "cases"],
              "variable": [
                {
                  "key": "internshipId",
                  "value": "{{lastInternshipId}}",
                  "description": "Internship ID from previous request"
                }
              ]
            },
            "description": "Create case with detailed clinical content. Will be automatically ingested into Python/Pinecone."
          },
          "response": []
        },
        {
          "name": "2. Get all cases for internship",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{token}}"
              }
            ],
            "url": {
              "raw": "{{baseUrl}}/api/internship/:internshipId/cases",
              "host": ["{{baseUrl}}"],
              "path": ["api", "internship", ":internshipId", "cases"],
              "variable": [
                {
                  "key": "internshipId",
                  "value": "{{lastInternshipId}}"
                }
              ]
            },
            "description": "Get all cases for specific internship, ordered by sequence"
          },
          "response": []
        },
        {
          "name": "3. Get case by ID",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{token}}"
              }
            ],
            "url": {
              "raw": "{{baseUrl}}/api/internship/cases/:caseId",
              "host": ["{{baseUrl}}"],
              "path": ["api", "internship", "cases", ":caseId"],
              "variable": [
                {
                  "key": "caseId",
                  "value": "{{lastCaseId}}",
                  "description": "Case MongoDB ObjectId"
                }
              ]
            },
            "description": "Get detailed case information including all content"
          },
          "response": []
        },
        {
          "name": "4. Update case",
          "request": {
            "method": "PATCH",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{token}}"
              },
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"patient_simulation_config\": {\n    \"patient_profile\": {\n      \"name\": \"John Smith\",\n      \"age\": 35,\n      \"gender\": \"male\",\n      \"condition\": \"Major Depressive Disorder\",\n      \"severity\": \"moderate\",\n      \"duration\": \"6 weeks\",\n      \"triggers\": [\"job loss\", \"financial stress\", \"loss of daily routine\"],\n      \"symptoms\": [\"depressed mood\", \"anhedonia\", \"early morning awakening\", \"decreased appetite\", \"psychomotor retardation\", \"difficulty concentrating\", \"feelings of worthlessness\"],\n      \"history\": \"One previous depressive episode 5 years ago, successfully treated with sertraline 100mg\",\n      \"medication\": \"Previously on sertraline 100mg\",\n      \"social_context\": \"Married, two children, recently unemployed\",\n      \"chief_complaint\": \"I just don't feel like myself anymore\",\n      \"presentation_style\": \"withdrawn, soft-spoken\"\n    },\n    \"scenario_type\": \"initial_clinical_interview\",\n    \"difficulty_level\": \"intermediate\",\n    \"interview_focus\": \"assessment_and_diagnosis\",\n    \"patient_openness\": \"moderately_forthcoming\"\n  }\n}",
              "options": {
                "raw": {
                  "language": "json"
                }
              }
            },
            "url": {
              "raw": "{{baseUrl}}/api/internship/cases/:caseId",
              "host": ["{{baseUrl}}"],
              "path": ["api", "internship", "cases", ":caseId"],
              "variable": [
                {
                  "key": "caseId",
                  "value": "{{lastCaseId}}"
                }
              ]
            },
            "description": "Update case. Will automatically re-ingest into Python/Pinecone."
          },
          "response": []
        },
        {
          "name": "5. Delete case",
          "request": {
            "method": "DELETE",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{token}}"
              }
            ],
            "url": {
              "raw": "{{baseUrl}}/api/internship/cases/:caseId",
              "host": ["{{baseUrl}}"],
              "path": ["api", "internship", "cases", ":caseId"],
              "variable": [
                {
                  "key": "caseId",
                  "value": "{{lastCaseId}}"
                }
              ]
            },
            "description": "Soft delete case. Also removes from Python/Pinecone."
          },
          "response": []
        },
        {
          "name": "6. Update case sequence",
          "request": {
            "method": "PATCH",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{token}}"
              },
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"item_id\": \"{{lastCaseId}}\",\n  \"sequence\": 2\n}",
              "options": {
                "raw": {
                  "language": "json"
                }
              }
            },
            "url": {
              "raw": "{{baseUrl}}/api/internship/cases/:caseId/sequence",
              "host": ["{{baseUrl}}"],
              "path": ["api", "internship", "cases", ":caseId", "sequence"],
              "variable": [
                {
                  "key": "caseId",
                  "value": "{{lastCaseId}}"
                }
              ]
            },
            "description": "Reorder case within internship"
          },
          "response": []
        }
      ]
    },
    {
      "name": "üîç Quick ID Retrieval - GET Endpoints",
      "item": [
        {
          "name": "Get All Sessions (by student)",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{studentToken}}"
              }
            ],
            "url": {
              "raw": "{{baseUrl}}/api/internship/sessions/:sessionId",
              "host": ["{{baseUrl}}"],
              "path": ["api", "internship", "sessions", ":sessionId"],
              "variable": [
                {
                  "key": "sessionId",
                  "value": "{{lastSessionId}}",
                  "description": "Use this to get session details and all messages"
                }
              ]
            },
            "description": "Get session details including all messages. Replace sessionId with actual ID from create session response."
          },
          "response": []
        },
        {
          "name": "Get Feedback by ID",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{token}}"
              }
            ],
            "url": {
              "raw": "{{baseUrl}}/api/internship/feedback/pending?page=1&limit=20",
              "host": ["{{baseUrl}}"],
              "path": ["api", "internship", "feedback", "pending"],
              "query": [
                {
                  "key": "page",
                  "value": "1"
                },
                {
                  "key": "limit",
                  "value": "20"
                }
              ]
            },
            "description": "Get all pending feedback - use this to find feedback IDs"
          },
          "response": []
        },
        {
          "name": "Get Feedback for Specific Case",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{studentToken}}"
              }
            ],
            "url": {
              "raw": "{{baseUrl}}/api/internship/cases/:caseId/feedback",
              "host": ["{{baseUrl}}"],
              "path": ["api", "internship", "cases", ":caseId", "feedback"],
              "variable": [
                {
                  "key": "caseId",
                  "value": "{{lastCaseId}}",
                  "description": "Case ID to get feedback for"
                }
              ]
            },
            "description": "Get all validated feedback for a specific case"
          },
          "response": []
        },
        {
          "name": "Get Student Logbook",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{studentToken}}"
              }
            ],
            "url": {
              "raw": "{{baseUrl}}/api/internship/:internshipId/logbook",
              "host": ["{{baseUrl}}"],
              "path": ["api", "internship", ":internshipId", "logbook"],
              "variable": [
                {
                  "key": "internshipId",
                  "value": "{{lastInternshipId}}"
                }
              ]
            },
            "description": "Get student's logbook with all entries"
          },
          "response": []
        }
      ]
    },
    {
      "name": "Student - Session Management",
      "item": [
        {
          "name": "1. Create session (Start patient interview)",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{studentToken}}",
                "description": "Use student JWT token"
              },
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"internship_id\": \"{{lastInternshipId}}\",\n  \"case_id\": \"{{lastCaseId}}\",\n  \"session_type\": \"patient_interview\"\n}",
              "options": {
                "raw": {
                  "language": "json"
                }
              }
            },
            "url": {
              "raw": "{{baseUrl}}/api/internship/sessions",
              "host": ["{{baseUrl}}"],
              "path": ["api", "internship", "sessions"]
            },
            "description": "Start patient interview session. Session types: patient_interview, therapist_consultation, supervisor_feedback"
          },
          "response": []
        },
        {
          "name": "2. Send message to AI patient",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{studentToken}}"
              },
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"message\": \"Hello John, thank you for coming in today. Can you tell me what brings you here?\"\n}",
              "options": {
                "raw": {
                  "language": "json"
                }
              }
            },
            "url": {
              "raw": "{{baseUrl}}/api/internship/sessions/:sessionId/message",
              "host": ["{{baseUrl}}"],
              "path": ["api", "internship", "sessions", ":sessionId", "message"],
              "variable": [
                {
                  "key": "sessionId",
                  "value": "{{lastSessionId}}",
                  "description": "Session ID from create session response"
                }
              ]
            },
            "description": "Send message to AI (patient/therapist/supervisor). May receive real-time tips."
          },
          "response": []
        },
        {
          "name": "3. Get session details",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{studentToken}}"
              }
            ],
            "url": {
              "raw": "{{baseUrl}}/api/internship/sessions/:sessionId",
              "host": ["{{baseUrl}}"],
              "path": ["api", "internship", "sessions", ":sessionId"],
              "variable": [
                {
                  "key": "sessionId",
                  "value": "{{lastSessionId}}"
                }
              ]
            },
            "description": "Get session details including all messages and real-time tips"
          },
          "response": []
        },
        {
          "name": "4. Complete session",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{studentToken}}"
              },
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{}",
              "options": {
                "raw": {
                  "language": "json"
                }
              }
            },
            "url": {
              "raw": "{{baseUrl}}/api/internship/sessions/:sessionId/complete",
              "host": ["{{baseUrl}}"],
              "path": ["api", "internship", "sessions", ":sessionId", "complete"],
              "variable": [
                {
                  "key": "sessionId",
                  "value": "{{lastSessionId}}"
                }
              ]
            },
            "description": "Mark session as completed. Required before generating feedback."
          },
          "response": []
        }
      ]
    },
    {
      "name": "Feedback Management",
      "item": [
        {
          "name": "1. Generate AI feedback (Student)",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{studentToken}}"
              },
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{}",
              "options": {
                "raw": {
                  "language": "json"
                }
              }
            },
            "url": {
              "raw": "{{baseUrl}}/api/internship/sessions/:sessionId/feedback",
              "host": ["{{baseUrl}}"],
              "path": ["api", "internship", "sessions", ":sessionId", "feedback"],
              "variable": [
                {
                  "key": "sessionId",
                  "value": "{{lastSessionId}}"
                }
              ]
            },
            "description": "Generate AI feedback for completed session. Status: PENDING_VALIDATION"
          },
          "response": []
        },
        {
          "name": "2. Get pending feedback (Professor)",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{token}}",
                "description": "Professor/Admin token"
              }
            ],
            "url": {
              "raw": "{{baseUrl}}/api/internship/feedback/pending?page=1&limit=10",
              "host": ["{{baseUrl}}"],
              "path": ["api", "internship", "feedback", "pending"],
              "query": [
                {
                  "key": "page",
                  "value": "1"
                },
                {
                  "key": "limit",
                  "value": "10"
                }
              ]
            },
            "description": "Get all feedback awaiting professor validation"
          },
          "response": []
        },
        {
          "name": "3. Validate feedback (Professor)",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{token}}"
              },
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"is_approved\": true,\n  \"professor_comments\": \"Excellent work on establishing rapport with the patient. Your empathy and active listening skills were evident throughout the interview. Good use of open-ended questions to explore the patient's concerns. Consider asking more about the timeline of symptoms in future interviews.\",\n  \"edited_score\": 88\n}",
              "options": {
                "raw": {
                  "language": "json"
                }
              }
            },
            "url": {
              "raw": "{{baseUrl}}/api/internship/feedback/:feedbackId/validate",
              "host": ["{{baseUrl}}"],
              "path": ["api", "internship", "feedback", ":feedbackId", "validate"],
              "variable": [
                {
                  "key": "feedbackId",
                  "value": "{{lastFeedbackId}}",
                  "description": "Feedback ID from pending list"
                }
              ]
            },
            "description": "Approve or revise AI feedback with professor comments"
          },
          "response": []
        },
        {
          "name": "4. Update feedback (Professor)",
          "request": {
            "method": "PATCH",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{token}}"
              },
              {
                "key": "Content-Type",
